{
  "name": "@wearzdk/eslint-config-prettier",
  "version": "1.1.2",
  "packageManager": "pnpm@8.15.6",
  "description": "ESLint config for React projects with Prettier",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/wearzdk/eslint-config-prettier#readme",
  "bugs": {
    "url": "https://github.com/wearzdk/eslint-config-prettier/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wearzdk/eslint-config-prettier.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^14.1.4",
    "@unocss/eslint-plugin": "^0.58.9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsonc": "^2.15.0",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-perfectionist": "^2.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-unicorn": "^52.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-yml": "^1.14.0",
    "globals": "^15.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.5.0",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.4",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "sucrase": "^3.35.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
